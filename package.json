{
    "name": "hongqi",
    "version": "1.2.0",
    "description": "红旗直播x32",
    "main": "main.js",
    "scripts": {
        "start": "node ./build/index.js hongqi development && electron .",
        "dist-win": "electron-builder -w --ia32",
        "dist-mac": "electron-builder -m",
        "build": "electron-packager .",
        "build-b": "electron-builder .",
        "dev:win:hq64": "node ./build/index.js hongqi 64 development && electron .",
        "dev:win:bt64": "node ./build/index.js benteng 64 development && electron .",
        "dev:win:wmkt64": "node ./build/index.js weimaketang 64 development && electron .",
        "test:mac:hq": "node ./build/index.js hongqi test && electron-builder -m",
        "test:mac:bt": "node ./build/index.js benteng test && electron-builder -m",
        "test:mac:wmkt": "node ./build/index.js weimaketang test && electron-builder -m",
        "test:win:hq64": "node ./build/index.js hongqi test 64 && electron-builder .",
        "test:win:bt64": "node ./build/index.js benteng test 64 && electron-builder .",
        "test:win:wmkt64": "node ./build/index.js weimaketang test 64 && electron-builder .",
        "test:win:hq32": "node ./build/index.js hongqi test 32 && electron-builder  -w --ia32",
        "test:win:bt32": "node ./build/index.js benteng test 32 && electron-builder -w --ia32",
        "test:win:wmkt32": "node ./build/index.js weimaketang test 32 && electron-builder -w --ia32",
        "prod:mac:hq": "node ./build/index.js hongqi production && electron-builder -m",
        "prod:mac:bt": "node ./build/index.js benteng production && electron-builder -m",
        "prod:mac:wmkt": "node ./build/index.js weimaketang production && electron-builder -m",
        "prod:win:hq64": "node ./build/index.js hongqi production 64 && electron-builder . --publish never",
        "prod:win:bt64": "node ./build/index.js benteng production 64 && electron-builder .",
        "prod:win:wmkt64": "node ./build/index.js weimaketang production 64 && electron-builder .",
        "prod:win:hq32": "node ./build/index.js hongqi production 32 && electron-builder  -w --ia32 --publish never",
        "prod:win:bt32": "node ./build/index.js benteng production 32 && electron-builder -w --ia32",
        "prod:win:wmkt32": "node ./build/index.js weimaketang production 32 && electron-builder -w --ia32",
        "test:win:all": "npm run test:win:bt64 && npm run test:win:hq64 && npm run test:win:wmkt64 && npm run test:win:bt32 && npm run test:win:hq32 && npm run test:win:wmkt32",
        "prod:win:all": "npm run prod:win:bt64 && npm run prod:win:hq64 && npm run prod:win:wmkt64 && npm run prod:win:bt32 && npm run prod:win:hq32 && npm run prod:win:wmkt32",
        "upload": "node ./uploadOSS.js"
    },
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "hongqics",
    "license": "CC0-1.0",
    "devDependencies": {
        "ali-oss": "^6.12.0",
        "electron": "^5.0.13",
        "electron-builder": "^20.44.4"
    },
    "dependencies": {
        "node-sass": "^4.11.0"
    },
    "build": {
        "extends": "./devtools/electron-builder.json",
        "mac": {
            "icon": "build/icons/icon.icns",
            "target": "pkg"
        },
        "win": {
            "icon": "build/icons/icon.ico",
            "requestedExecutionLevel": "highestAvailable",
            "target": "nsis"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    }
}